<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<body>
    <referenceBlock name="customer_account_navigation">
        <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-customlearning-link">
            <arguments>
                <argument name="path" xsi:type="string">auction/index/mybids</argument>
                <argument name="label" xsi:type="string" translate="true">My Auction</argument>
            </arguments>
        </block>
    </referenceBlock>
    <referenceContainer name="sidebar.main">
        <block class="Magestore\Auction\Block\Package" name="custmer.auction.package.name">
            <action method="showPackageName" />
        </block>
    </referenceContainer>
</body>
</page>